{
  "manifest_version": 3,
  "name": "Weather Forecast Extension",
  "version": "1.0",
  "description": "Get the weather forecast for any city",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "geolocation",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    
    "128": "icons/icon128.png"
  }
}
